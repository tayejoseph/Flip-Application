{
  "name": "cardvestmobileapplication",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 12 Pro Max'",
    "android:test-linking": "adb shell am start -W -a android.intent.action.VIEW -d \"https://flip.com/app\" com.cardvestmobileapplication",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "ios:pod-i": "cd ios && pod install && cd .. && yarn ios",
    "ios:pod-u": "cd ios && pod update && cd .. && yarn ios",
    "clean:android": "cd android && ./gradlew app:clean && cd ..",
    "clean:ios": "cd ios && xcodebuild clean && cd ..",
    "react-devtools": "react-devtools",
    "release:build": "yarn android --variant=release",
    "clean:android-build": "cd android && ./gradlew app:clean && ./gradlew app:assembleRelease && cd ..",
    "clean:android-bundle": "cd android && ./gradlew app:clean && ./gradlew bundleRelease && cd .."
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/hooks": "^2.6.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/picker": "^1.8.1",
    "@react-navigation/drawer": "^5.9.0",
    "@react-navigation/material-bottom-tabs": "^5.2.16",
    "@react-navigation/native": "^5.7.3",
    "@react-navigation/stack": "^5.9.0",
    "axios": "^0.19.2",
    "react": "16.13.1",
    "react-native": "0.63.2",
    "react-native-communications": "^2.2.1",
    "react-native-flash-message": "^0.1.22",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-paper": "^4.2.0",
    "react-native-raw-bottom-sheet": "^2.2.0",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area": "^0.5.1",
    "react-native-safe-area-context": "^3.1.4",
    "react-native-screens": "^2.10.1",
    "react-native-simple-encryption": "^1.2.1",
    "react-native-simple-toast": "^1.1.2",
    "react-native-svg": "^12.1.0",
    "react-native-tab-view": "^2.15.2",
    "react-native-vector-icons": "^7.0.0",
    "react-native-webview": "^11.2.1",
    "react-navigation": "^4.4.0",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.5.1",
    "jest": "^25.1.0",
    "jetifier": "^1.6.6",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "^2.0.5",
    "react-devtools": "^4.8.2",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
